{
  "name": "mood-logger",
  "version": "1.0.0",
  "description": "A mood logging application with edit and delete functionality",
  "main": "script.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "python3 -m http.server 8000"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "collectCoverageFrom": [
      "script.js",
      "!**/node_modules/**"
    ]
  },
  "keywords": ["mood", "logger", "journal", "emotions"],
  "author": "Group 2",
  "license": "MIT"
} 